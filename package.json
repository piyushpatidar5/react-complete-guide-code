{
  "name": "phoenix",
  "sideEffects": false,
  "private": true,
  "version": "0.0.1",
  "description": "Phoenix(Hybrid Workloads) project for backup/restore of servers, vms, ec2, and many more.",
  "main": " ",
  "scripts": {
    "test": "jest --verbose",
    "jslint": "eslint",
    "csslint": "stylelint",
    "build": "gulp build & webpack --config ./webpack.prod.js",
    "devserver": "gulp dev & webpack-dev-server --config ./webpack.dev.server.js --host 0.0.0.0 --hot",
    "vmdevserver": "sh ./webpack-cert.sh && npm run devserver -- --env vm",
    "dev": "gulp dev & webpack --config ./webpack.dev.js",
    "devwatch": "gulp dev & webpack --config ./webpack.dev.js --mode development --watch --progress",
    "csswatch": "gulp sass-watch",
    "pyut": "python src/roboqa/testcase/qa/UITest/ui_test*.py -f",
    "font-generate": "node ./generateFont.js",
    "generate": "plop --plopfile src/cportal/cportalserver/rjs/codeGenerator/plopfile.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.druva.org/phoenixdev/phoenix.git"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=6.10.0"
  },
