    "@amcharts/amcharts4-geodata": "4.1.17",
    "@babel/runtime": "7.8.4",
    "@druva-apps/orion-icons": "1.0.22",
    "abortcontroller-polyfill": "1.4.0",
    "bootstrap": "4.4.1",
    "cronstrue": "1.68.0",
    "intersection-observer": "0.10.0",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.25",
    "orion": "3.0.6",
    "prop-types": "15.6.2",
    "react": "16.13.1",
    "react-beautiful-dnd": "10.0.4",
    "react-copy-to-clipboard": "5.0.1",
    "react-csv": "2.0.3",
    "react-dom": "16.13.1",
    "react-hashchange": "0.1.0",
    "react-hot-loader": "4.12.19",
    "react-intersection-observer": "8.26.2",
    "react-loading-skeleton": "2.2.0",
    "react-perfect-scrollbar": "1.1.1",
    "react-redux": "7.2.4",
    "react-virtualized": "9.20.1",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
    "stacktrace-js": "2.0.0",
