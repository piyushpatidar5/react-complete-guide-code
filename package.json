    "webpack-dev-server": "4.7.3",
    "webpack-merge": "4.1.2",
    "webpack-subresource-integrity": "5.1.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/cportal/cportalserver/rjs/setupTests.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(png|svg|jpg|gif|pdf|jpeg)$": "<rootDir>/src/cportal/cportalserver/rjs/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "./vmware/client/proxydeployment/ui/phoenixproxyclient/",
      "./commonclients/phoenixapp/gui/",
      "./src/cportal/cportalserver/rjs/awsResources/",
      "./src/cportal/cportalserver/rjs/codeGenerator"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/cportal/cportalserver/rjs/**/*.js",
      "!**/__test__/**",
      "!**/awsResources/**",
      "!**/codeGenerator/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(orion|lodash-es))/",
      "node_modules/yup"
    ]
  }
}