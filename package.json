    "gulp-html-replace": "1.6.2",
    "gulp-sass": "5.0.0",
    "gulp-sri": "0.3.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "25.5.2",
    "jest-fetch-mock": "1.5.0",
    "mini-css-extract-plugin": "2.5.3",
