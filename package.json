    "@hot-loader/react-dom": "16.13.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.5.1",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "3.4.2",
    "css-minimizer-webpack-plugin": "3.4.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
